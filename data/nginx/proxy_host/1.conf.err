# ------------------------------------------------------------
# jenkins.paulryu93.ddns.net
# DO NOT EDIT THIS FILE DIRECTLY, CHANGES WILL BE LOST WHEN UPDATING!
# ------------------------------------------------------------


server {
  set $forward_scheme "https";
  set $server         "http:";
  set $port           "8080";
  set $forward_path   "//jenkins-server";


  listen 0.0.0.0:80;
  listen [::]:80;



  listen 0.0.0.0:443 ssl;
  listen [::]:443 ssl;



  server_name jenkins.paulryu93.ddns.net;



  # Certbot TLS
  ssl_certificate /data/tls/certbot/live/npm-1/fullchain.pem;
  ssl_certificate_key /data/tls/certbot/live/npm-1/privkey.pem;
  
















  

  include conf.d/include/always.conf;

  




  location / {
    
    
    
    include conf.d/include/proxy-headers.conf;
    proxy_pass https://http::8080//jenkins-server;
    
  }


# custom locations


  # Custom
  include /data/custom_nginx/server_proxy.conf;
}

